{
    "manifest_version": 2,
    "name": "show outlook Appointments",
    "description": "An extension to show outlook Appointments",
    "applications": {
        "gecko": {
            "id": "soa@coolshou.idv.tw",
            "strict_min_version": "78.0"
        }
    },
    "author": "coolshou",
    "version": "0.5.0",
    "icons": {
        "64": "images/icon-64px.png",
        "32": "images/icon-32px.png",
        "16": "images/icon-16px.png"
    },
    "background": {
        "scripts": [
            "src/background.js"
        ]
    },
    "permissions": [
        "messagesModify",
        "messagesRead"
    ]
}